{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllSkyCamera": {
    "CacheImageSaveRoot": "/tmp/hvo-skymonitor-cache",
    "UseImageCircleMask": true,
    "MaxAttemptedFps": 1.5,
    "ImageTextFontSize": 20.0,
    "CleanCacheOnStart": true
  },
  "AllSkyImageSave": {
    "ImageSaveRoot": "/tmp/hvo-skymonitor-images",
    "MaxImageAgeSeconds": 90
  },
  "AllSkyImageCleanup": {
    "MaxHoursToKeep": 6
  },
  "AllSkyTimelapse": {
    "FFMpegPath": "/usr/local/bin/ffmpeg",
    "ImageSaveRoot": "/tmp/hvo-skymonitor-images",
    "FFMpegVideoFps": 30,
    "OutputPrefix": "DevAllSkyCam"
  }
}
