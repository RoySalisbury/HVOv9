{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllSkyCamera": {
    "RestartOnFailureWaitTimeSeconds": 15,
    "CacheImageSaveRoot": "/dev/shm/skymonitor",
    "AutoExposureMaxDurationMs": 20000,
    "AutoExposureMaxGain": 200,
    "ExposureBrightness": 50,
    "AutoExposureBrightness": true,
    "AutoExposureTargetBrightness": 100,
    "ExposureGain": 0,
    "AutoExposureGain": true,
    "ExposureDurationMs": 0,
    "AutoExposureDuration": true,
    "ImageCircleRotationAngle": 0.0,
    "ImageCircleDiameter": 1088,
    "ImageCircleOffsetX": 368,
    "ImageCircleOffsetY": 8,
    "ImageHeight": 960,
    "ImageWidth": 1704,
    "ImageTextFontSize": 17.5,
    "MaxAttemptedFps": 2.15,
    "JpegImageQuality": 90,
    "ImageBinMode": 1,
    "ImageType": "ASI_IMG_RAW16",
    "CameraIndex": 0,
    "CameraRoiWidth": 0,
    "CameraRoiHeight": 0,
    "UseImageCircleMask": true,
    "CleanCacheOnStart": true,
    "LatestImageFileName": "latestImage.jpg"
  },
  "AllSkyImageSave": {
    "RestartOnFailureWaitTimeSeconds": 15,
    "ImageSaveRoot": "/home/pi/skymonitor",
    "MaxImageAgeSeconds": 60
  },
  "AllSkyImageCleanup": {
    "RestartOnFailureWaitTimeSeconds": 15,
    "MaxHoursToKeep": 3
  },
  "AllSkyTimelapse": {
    "RestartOnFailureWaitTimeSeconds": 15,
    "FFMpegPath": "/usr/bin/ffmpeg",
    "FFMpegOutputArgs": "-c:v libx264 -pix_fmt yuv420p -preset fast",
    "FFMpegVideoFps": 60,
    "ImageSaveRoot": "/home/pi/skymonitor",
    "OutputPrefix": "AllSkyCam01"
  }
}
